{
  "name": "medusa-file-cloudflare-images",
  "version": "1.0.0",
  "description": "Cloudflare images file connector for Medusa Commerce applications",
  "main": "index.js",
  "author": "Drew DeCarme (drewdecarme@gmail.com)",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "bundle": "esbuild src/services/** --outdir=services --format=cjs --target=esnext",
    "bundle-swc": "swc src -d .",
    "build": "yarn bundle",
    "build:ci": "yarn bundle",
    "prepare": "yarn build",
    "dev": "yarn bundle --watch",
    "test": "jest",
    "package": "yarn build && yarn np --no-tests --no-yarn"
  },
  "peerDependencies": {
    "medusa-interfaces": "1.x"
  },
  "files": [
    "services"
  ],
  "devDependencies": {
    "@types/formidable": "^2.0.3",
    "chokidar": "^3.5.2",
    "client-sessions": "^0.8.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.10",
    "eslint": "^8.6.0",
    "jest": "^27.4.7",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "body-parser": "^1.19.1",
    "express": "^4.17.2",
    "form-data": "^4.0.0",
    "formidable": "^2.0.1",
    "medusa-core-utils": "^1.1.31",
    "medusa-interfaces": "^1.1.32",
    "medusa-test-utils": "^1.1.36",
    "node-fetch": "2.6.6",
    "np": "^7.6.0",
    "regenerator-runtime": "^0.13.9",
    "typeorm": "^0.2.41"
  },
  "packageManager": "yarn@3.1.1"
}
